<?xml version="1.0" encoding="utf-8"?>
<shape
xmlns:android="http://schemas.android.com/apk/res/android"
android:shape="rectangle"><!--设置形状为矩形 -->
<!-- 填充的颜色 -->
<solid android:color="#FFFFFF"/>
 <stroke android:dashWidth="2dp" android:dashGap="1dp"

                android:width="2dp" android:color="#c3c3c3"></stroke>
 
<!-- 设置矩形的四个角为弧形 -->
<!-- radius为弧形的半径 -->
<corners android:radius="14dip"/>
</shape>

